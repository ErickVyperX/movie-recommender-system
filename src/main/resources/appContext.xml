<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="contentBasedFilter" class="io.datajek.spring.basics.movierecommendersystem.otherpackage.ContentBasedFilter">
    </bean>

    <bean id="collaborativeFilter" class="io.datajek.spring.basics.movierecommendersystem.otherpackage.CollaborativeFilter">
    </bean>

    <bean id="recommederImplementation" class="io.datajek.spring.basics.movierecommendersystem.otherpackage.RecommenderImplementation">
        <constructor-arg ref="collaborativeFilter"/>
    </bean>
</beans>